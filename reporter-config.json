{
  "reporterEnabled": "spec, mocha-junit-reporter, mochawesome, mocha-allure-reporter",
  "mochaJunitReporterReporterOptions": {
    "mochaFile": "cypress/results/[hash].xml"
  },
  "mochawesomeReporterOptions": {
    "files": ["cypress/tmp-e2e-json/*.json"],
    "overwrite": false,
    "html": false,
    "json": true
  }
}
