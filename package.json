{
  "name": "ngx-sample",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "cypress-open": "cypress open",
    "cypress-run": "cypress run",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --runInBand",
    "lint": "ng lint",
    "packagr": "ng-packagr -p ng-package.json",
    "ghpages": "ng build --prod --aot --no-progress --base-href '/ngx-sample/' && ngh -S --dir dist/",
    "prettier": "npm run prettier-ts && npm run prettier-html && npm run prettier-scss",
    "prettier-ts": "prettier --single-quote --parser typescript --write \"./**/*.ts\"",
    "prettier-html": "prettier --parser angular --write \"./**/*.component.html\"",
    "prettier-scss": "prettier --write \"./**/*.scss\"",
    "e2e": "ng e2e",
    "ci:e2e:server": "ng serve --no-progress",
    "ci:e2e:cypress": "cypress run",
    "ci:e2e:cypress:actual": "cypress run --env type=actual",
    "ci:e2e:cypress:base": "cypress run --env type=base --config screenshotsFolder=cypress/snapshots/base",
    "ci:e2e:run:actual": "wait-on http-get://localhost:4200 && npm run ci:e2e:cypress:actual",
    "ci:e2e:run:base": "wait-on http-get://localhost:4200 && npm run ci:e2e:cypress:base",
    "test:watch": "jest --watch"
  },
  "private": false,
  "dependencies": {
    "@angular-builders/jest": "7.4.2",
    "@angular/animations": "7.2.15",
    "@angular/cdk": "7.3.7",
    "@angular/common": "7.2.15",
    "@angular/compiler": "7.2.15",
    "@angular/core": "7.2.15",
    "@angular/forms": "7.2.15",
    "@angular/http": "7.2.15",
    "@angular/platform-browser": "7.2.15",
    "@angular/platform-browser-dynamic": "7.2.15",
    "@angular/platform-server": "7.2.15",
    "@angular/router": "7.2.15",
    "@briebug/cypress-schematic": "2.0.0",
    "@briebug/jest-schematic": "1.7.0",
    "@types/jest": "24.0.13",
    "ag-grid-angular": "20.2.0",
    "ag-grid-community": "20.2.0",
    "ag-grid-enterprise": "20.2.0",
    "angular-google-auth2": "1.0.8",
    "bluebird": "3.5.4",
    "bootstrap": "4.3.1",
    "chart.js": "2.8.0",
    "core-js": "2.6.5",
    "cypress": "3.3.0",
    "font-awesome": "4.7.0",
    "license-webpack-plugin": "2.1.1",
    "ngx-bootstrap": "4.1.1",
    "node-sass": "4.12.0",
    "primeng": "7.1.3",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.5.2",
    "tslib": "1.9.3",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.9",
    "@angular/cli": "7.3.9",
    "@angular/compiler-cli": "7.2.15",
    "@angular/language-service": "7.2.15",
    "@cypress/webpack-preprocessor": "4.0.3",
    "@types/jasmine": "3.3.12",
    "@types/jasminewd2": "2.0.6",
    "@types/moment": "2.13.0",
    "@types/moment-range": "3.1.2",
    "@types/node": "6.14.6",
    "@types/reveal": "3.3.33",
    "angular-cli-ghpages": "0.5.3",
    "codelyzer": "5.0.1",
    "cypress": "3.3.0",
    "cypress-visual-regression": "1.0.0",
    "cypress-xpath": "1.4.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "24.8.0",
    "jest-preset-angular": "7.1.0",
    "karma-cli": "2.0.0",
    "moment": "2.24.0",
    "moment-range": "4.0.2",
    "ng-packagr": "5.2.0",
    "prettier": "1.17.1",
    "reveal.js": "3.6.0",
    "rxjs-tslint": "0.1.7",
    "start-server-and-test": "1.9.0",
    "ts-loader": "6.0.1",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "typescript": "3.2.4",
    "wait-on": "3.2.0"
  }
}
