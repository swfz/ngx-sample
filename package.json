{
  "name": "ngx-sample",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "cypress-open": "cypress open",
    "cypress-run": "cypress run",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --runInBand",
    "lint": "ng lint",
    "packagr": "ng-packagr -p ng-package.json",
    "ghpages": "ng build --prod --aot --no-progress --base-href '/ngx-sample/' && ngh -S -m '[skip ci] deploy to GitHub pages' --dir dist/",
    "ngh": "ngh -m '[skip ci] deploy to GitHub pages' --dir dist/",
    "prettier": "prettier --single-quote --write './**/*.{scss,ts,html}'",
    "prettier:check": "prettier --single-quote --check './**/*.{scss,ts,html}'",
    "e2e": "ng e2e",
    "ci:e2e:server": "ng serve --no-progress",
    "ci:e2e:cypress": "cypress run",
    "ci:e2e:cypress:actual": "cypress run --env type=actual --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json",
    "ci:e2e:cypress:base": "cypress run --env type=base --config screenshotsFolder=cypress/snapshots/base --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json",
    "ci:e2e:run:actual": "wait-on http-get://localhost:4200 && npm run ci:e2e:cypress:actual",
    "ci:e2e:run:base": "wait-on http-get://localhost:4200 && npm run ci:e2e:cypress:base",
    "ci:e2e:report:json": "mochawesome-merge --reportDir cypress/tmp-e2e-json > mochawesome.json",
    "ci:e2e:report:html": "marge mochawesome.json",
    "allure": "allure generate allure-results -o allure-report",
    "test:watch": "jest --watch"
  },
  "private": false,
  "dependencies": {
    "@angular-builders/jest": "9.0.1",
    "@angular/animations": "9.1.13",
    "@angular/cdk": "9.2.4",
    "@angular/common": "9.1.13",
    "@angular/compiler": "9.1.13",
    "@angular/core": "9.1.13",
    "@angular/forms": "9.1.13",
    "@angular/platform-browser": "9.1.13",
    "@angular/platform-browser-dynamic": "9.1.13",
    "@angular/platform-server": "9.1.13",
    "@angular/router": "9.1.13",
    "@briebug/cypress-schematic": "3.3.0",
    "@briebug/jest-schematic": "2.2.0",
    "@fullcalendar/core": "4.4.2",
    "@types/gtag.js": "0.0.4",
    "@types/jest": "26.0.19",
    "@types/lodash": "4.14.166",
    "ag-grid-angular": "23.2.1",
    "ag-grid-community": "23.2.1",
    "ag-grid-enterprise": "23.2.1",
    "allure-commandline": "2.13.0",
    "angular-cli-ghpages": "0.6.2",
    "angular-google-auth2": "1.0.8",
    "bluebird": "3.7.2",
    "bootstrap": "4.5.3",
    "chart.js": "2.9.4",
    "core-js": "2.6.12",
    "cypress": "4.5.0",
    "font-awesome": "4.7.0",
    "license-webpack-plugin": "2.3.9",
    "mocha-allure-reporter": "1.4.0",
    "ngx-bootstrap": "5.5.0",
    "node-sass": "4.14.1",
    "primeng": "9.1.3",
    "quill": "1.3.7",
    "rxjs": "6.6.3",
    "rxjs-compat": "6.6.3",
    "tslib": "1.14.1",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.11",
    "@angular/cli": "9.1.11",
    "@angular/compiler-cli": "9.1.13",
    "@angular/language-service": "9.1.13",
    "@cypress/webpack-preprocessor": "5.5.0",
    "@types/jasmine": "3.6.2",
    "@types/jasminewd2": "2.0.8",
    "@types/moment": "2.13.0",
    "@types/moment-range": "3.1.2",
    "@types/node": "12.19.11",
    "@types/reveal": "3.3.33",
    "codelyzer": "5.2.2",
    "cypress": "4.5.0",
    "cypress-visual-regression": "1.5.7",
    "cypress-xpath": "1.6.1",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "jest": "26.6.3",
    "jest-preset-angular": "7.1.1",
    "karma-cli": "2.0.0",
    "mocha": "7.2.0",
    "mocha-junit-reporter": "2.0.0",
    "mocha-multi-reporters": "1.5.1",
    "mochawesome": "6.2.1",
    "mochawesome-merge": "2.1.0",
    "mochawesome-report-generator": "5.1.0",
    "moment": "2.29.1",
    "moment-range": "4.0.2",
    "ng-packagr": "9.1.5",
    "prettier": "1.19.1",
    "reveal.js": "3.9.2",
    "rxjs-tslint": "0.1.8",
    "start-server-and-test": "1.11.7",
    "ts-loader": "6.2.2",
    "ts-node": "8.10.2",
    "tslint": "5.20.1",
    "typescript": "3.8.3",
    "wait-on": "5.2.1"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ]
  }
}
