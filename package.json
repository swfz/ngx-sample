{
  "name": "ngx-sample",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "cypress-open": "cypress open",
    "cypress-run": "cypress run",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --runInBand",
    "lint": "ng lint",
    "packagr": "ng-packagr -p ng-package.json",
    "ghpages": "ng build --prod --aot --no-progress --base-href '/ngx-sample/' && ngh -S -m '[skip ci] deploy to GitHub pages' --dir dist/",
    "ngh": "ngh -S -m '[skip ci] deploy to GitHub pages' --dir dist/",
    "prettier": "prettier --single-quote --write './**/*.{scss,ts,html}'",
    "prettier:check": "prettier --single-quote --check './**/*.{scss,ts,html}'",
    "e2e": "ng e2e",
    "ci:e2e:server": "ng serve --no-progress",
    "ci:e2e:cypress": "cypress run",
    "ci:e2e:cypress:actual": "cypress run --env type=actual --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json",
    "ci:e2e:cypress:base": "cypress run --env type=base --config screenshotsFolder=cypress/snapshots/base --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json",
    "ci:e2e:run:actual": "wait-on http-get://localhost:4200 && npm run ci:e2e:cypress:actual",
    "ci:e2e:run:base": "wait-on http-get://localhost:4200 && npm run ci:e2e:cypress:base",
    "ci:e2e:report:json": "mochawesome-merge --reportDir cypress/tmp-e2e-json > mochawesome.json",
    "ci:e2e:report:html": "marge mochawesome.json",
    "test:watch": "jest --watch"
  },
  "private": false,
  "dependencies": {
    "@angular-builders/jest": "8.0.4",
    "@angular/animations": "8.1.1",
    "@angular/cdk": "8.1.0",
    "@angular/common": "8.1.1",
    "@angular/compiler": "8.1.1",
    "@angular/core": "8.1.1",
    "@angular/forms": "8.1.1",
    "@angular/http": "7.2.15",
    "@angular/platform-browser": "8.1.1",
    "@angular/platform-browser-dynamic": "8.1.1",
    "@angular/platform-server": "8.1.1",
    "@angular/router": "8.1.1",
    "@briebug/cypress-schematic": "2.0.0",
    "@briebug/jest-schematic": "1.7.0",
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.136",
    "ag-grid-angular": "21.0.1",
    "ag-grid-community": "21.0.1",
    "ag-grid-enterprise": "21.0.1",
    "angular-google-auth2": "1.0.8",
    "bluebird": "3.5.5",
    "bootstrap": "4.3.1",
    "chart.js": "2.8.0",
    "core-js": "2.6.9",
    "cypress": "3.4.0",
    "font-awesome": "4.7.0",
    "license-webpack-plugin": "2.1.1",
    "ngx-bootstrap": "5.1.0",
    "node-sass": "4.12.0",
    "primeng": "8.0.1",
    "quill": "1.3.6",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.5.2",
    "tslib": "1.10.0",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.801.1",
    "@angular/cli": "8.1.1",
    "@angular/compiler-cli": "8.1.1",
    "@angular/language-service": "8.1.1",
    "@cypress/webpack-preprocessor": "4.1.0",
    "@types/jasmine": "3.3.13",
    "@types/jasminewd2": "2.0.6",
    "@types/moment": "2.13.0",
    "@types/moment-range": "3.1.2",
    "@types/node": "6.14.6",
    "@types/reveal": "3.3.33",
    "angular-cli-ghpages": "0.5.3",
    "codelyzer": "5.1.0",
    "cypress": "3.4.0",
    "cypress-visual-regression": "1.0.3",
    "cypress-xpath": "1.4.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "24.8.0",
    "jest-preset-angular": "7.1.1",
    "karma-cli": "2.0.0",
    "mocha": "5.2.0",
    "mocha-junit-reporter": "1.23.0",
    "mocha-multi-reporters": "1.1.7",
    "mochawesome": "3.1.2",
    "mochawesome-merge": "1.0.7",
    "mochawesome-report-generator": "3.1.5",
    "moment": "2.24.0",
    "moment-range": "4.0.2",
    "ng-packagr": "5.3.0",
    "prettier": "1.18.2",
    "reveal.js": "3.8.0",
    "rxjs-tslint": "0.1.7",
    "start-server-and-test": "1.9.1",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.4.5",
    "wait-on": "3.3.0"
  }
}
